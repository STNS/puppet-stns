{
  "name": "hfm-stns",
  "version": "2.0.0",
  "author": "hfm",
  "summary": "This puppet module install and configure STNS (Simple Toml Name Service).",
  "license": "MIT",
  "source": "https://github.com/STNS/puppet-stns",
  "project_page": "https://github.com/STNS/puppet-stns",
  "issues_url": "https://github.com/STNS/puppet-stns/issues",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.7.0 < 6.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 2.0.0"
    },
    {
      "name": "puppetlabs-concat",
      "version_requirement": ">= 2.0.0"
    }
  ],
  "data_provider": null,
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific Linux",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    }
  ]
}
